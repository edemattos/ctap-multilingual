<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="wrapper">
		<!-- Main section -->
		<section>
			<!-- Page content -->
			<div class="content-wrapper">
				<div class="content-heading">
					Feature Set Manager:
					<g:InlineLabel ui:field="featureSetName" />
				</div>


				<div class="row">
					<g:HTMLPanel ui:field="featureListsWrapper"
						styleName="col-lg-12">
						<!--selected feature list panel -->
						<g:HTMLPanel ui:field="selectedFeatureListPanel"
							styleName="col-lg-6">

							<div class="panel panel-success">
								<div class="panel-heading">
									Selected Features
								</div>
								<div class="panel-body">
									<div class="panel">
										<div class="panel-body">
											<g:Button ui:field="deleteSelected"
												styleName="btn mb-sm btn-danger btn-labeled" />

											<div class="btn-group mb-sm">
												<button type="button" data-toggle="dropdown"
													class="btn dropdown-toggle btn-warning btn-labeled">
													<span class="btn-label">
														<i class="fa fa-check-square-o" />
													</span>
													Select
													<span class="caret"></span>
												</button>
												<ul role="menu" class="dropdown-menu">
													<li>
														<g:Anchor ui:field="selectAll">All</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectDutch">Dutch</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectEnglish">English</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectGerman">German</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectPortuguese">Portuguese</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectClear">Clear</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectReverse">Reverse</g:Anchor>
													</li>
													<!-- add new language here -->
												</ul>
											</div>

											<g:Button
												styleName="btn mb-sm btn-success btn-labeled"
												ui:field="showAll">Show All</g:Button>
										</div>
									</div>
									<g:HTMLPanel ui:field="feedbackPanel"
										styleName="alert" visible="false">
										<em class="fa fa-exclamation-circle fa-lg fa-fw"></em>
										<g:InlineLabel ui:field="feedbackLabel">feedback</g:InlineLabel>
										<g:Button ui:field="closeFeedbackPanel"
											styleName="close" text="X" />
									</g:HTMLPanel>

									<p class="lead">List of Features Selected</p>
									<p>

										<form class="form-inline">

											Show
											<g:ListBox ui:field="nRecords" width="80px">
												<g:item value="10">10</g:item>
												<g:item value="20">20</g:item>
												<g:item value="30">30</g:item>
												<g:item value="40">40</g:item>
												<g:item value="50">50</g:item>
												<g:item value="100">100</g:item>
												<g:item value="all">all</g:item>
											</g:ListBox>
											records per page

											<div class="form-group has-feedback pull-right">
												<g:TextBox ui:field="search"
													styleName="form-control form-control-rounded input-sm" />
												<span
													class="fa fa-search form-control-feedback text-muted"></span>
											</div>
										</form>


									</p>
									<g:HTMLPanel
										ui:field="selectedFeatureCellTablePanel" />
								</div>
							</div>

						</g:HTMLPanel>

						<g:HTMLPanel ui:field="availableFeatureListPanel"
							styleName="col-lg-6">

							<div class="panel panel-info">
								<div class="panel-heading">
									Available Features
								</div>
								<div class="panel-body">


									<div class="panel">
										<div class="panel-body">
											<g:Button ui:field="addSelected"
												styleName="btn mb-sm btn-primary 
										btn-labeled" />

											<div class="btn-group mb-sm">
												<button type="button" data-toggle="dropdown"
													class="btn dropdown-toggle btn-warning btn-labeled">
													<span class="btn-label">
														<i class="fa fa-check-square-o" />
													</span>
													Select
													<span class="caret"></span>
												</button>
												<ul role="menu" class="dropdown-menu">
													<li>
														<g:Anchor ui:field="selectAllRightPanel">All</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectDutchRightPanel">Dutch</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectEnglishRightPanel">English</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectGermanRightPanel">German</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectPortugueseRightPanel">Portuguese</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectClearRightPanel">Clear</g:Anchor>
													</li>
													<li>
														<g:Anchor ui:field="selectReverseRightPanel">Reverse</g:Anchor>
													</li>
													<!-- add new language here -->
												</ul>
											</div>

											<g:Button
												styleName="btn mb-sm btn-success btn-labeled"
												ui:field="showAllRightPanel">Show All</g:Button>

										</div>
									</div>

									<g:HTMLPanel ui:field="feedbackPanelRightPanel"
										styleName="alert" visible="false">
										<em class="fa fa-exclamation-circle fa-lg fa-fw"></em>
										<g:InlineLabel
											ui:field="feedbackLabelRightPanel">feedback</g:InlineLabel>
										<g:Button ui:field="closeFeedbackPanelRightPanel"
											styleName="close" text="X" />
									</g:HTMLPanel>
									<!-- <div class="panel panel-info"> -->
									<!-- <div class="panel-body"> -->
									<p class="lead">List of Available Features</p>

									<p>
										<form class="form-inline">

											Show
											<g:ListBox ui:field="nRecordsRightPanel"
												width="80px">
												<g:item value="10">10</g:item>
												<g:item value="20">20</g:item>
												<g:item value="30">30</g:item>
												<g:item value="40">40</g:item>
												<g:item value="50">50</g:item>
												<g:item value="100">100</g:item>
												<g:item value="all">all</g:item>
											</g:ListBox>
											records per page

											<div class="form-group has-feedback pull-right">
												<g:TextBox ui:field="searchRightPanel"
													styleName="form-control form-control-rounded input-sm" />
												<span
													class="fa fa-search form-control-feedback text-muted"></span>
											</div>
										</form>
									</p>
									<g:HTMLPanel
										ui:field="availableFeatureCellTablePanel" />
								</div>
							</div>


						</g:HTMLPanel>
					</g:HTMLPanel> <!-- end of featureListsWrapper -->


					<!-- Details sidebar -->
					<g:HTMLPanel ui:field="aeDetailsPanel"
						styleName="col-lg-3" visible="false">
						<div class="panel panel-primary">
							<div class="panel-heading">
								Feature Details
							</div>
							<div class="panel-body">
								<div class="form-group">
									<p>Created on</p>
									<g:TextBox styleName="form-control" enabled="false"
										ui:field="aeCreatedOn" />
								</div>
								<div class="form-group">
									<p>Name</p>
									<g:TextBox styleName="form-control" ui:field="aeName"
										enabled="false" />
								</div>
								<div class="form-group">
									<p>Version</p>
									<g:TextBox styleName="form-control"
										ui:field="aeVersion" enabled="false" />
								</div>
								<div class="form-group">
									<p>Vendor</p>
									<g:TextBox styleName="form-control"
										ui:field="aeVendor" enabled="false" />
								</div>
								<div class="form-group">
									<p>
										Desc
										<!-- <g:Button ui:field="saveAEDetail" styleName="btn btn-sm btn-primary" -->
										<!-- text="Save" /> -->
										ription
									</p>
									<g:TextArea height="100px" styleName="form-control"
										ui:field="aeDescription" enabled="false" />
								</div>
								<g:Button ui:field="closeAEDetail"
									styleName="btn btn-sm" text="Close" />
							</div>
						</div>
					</g:HTMLPanel>

				</div> <!-- end of row -->
			</div>
		</section>

	</g:HTMLPanel>
</ui:UiBinder> 